import os
import vitaflow.playground.cifar10


#VitaFlowEngine
VitaFlowEngine.batch_size=8
VitaFlowEngine.num_epochs=100
VitaFlowEngine.dataset_class_with_path="vitaflow.playground.cifar10.cifar10_dataset.Cifar10Dataset"
VitaFlowEngine.iterator_class_with_path="vitaflow.playground.cifar10.cifar10_basic_iterator.Cifar10BasicIterator"
VitaFlowEngine.model_class_with_path="vitaflow.models.image.classification.naive_convnet.NaiveConvNet"
#(45000 // batch_size) // 5 = 112
VitaFlowEngine.save_checkpoints_steps=112
VitaFlowEngine.keep_checkpoint_max=5
